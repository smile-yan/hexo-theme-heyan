<div class="p-2 pe-4 d-none d-md-block">
    <% if (page.prev) { %> 
        <a href="<%- url_for(page.prev.path)  %>" class="btn btn-outline-secondary btn-sm float-md-left float-sm-right">
            <small>
                < 上一篇：<%- page.prev.title  %>
            </small>
        </a>
    <% } %> 
    <% if (page.next) { %> 
        <a href="<%- url_for(page.next.path)%>" class="btn btn-outline-secondary btn-sm float-end" %>>
            <small>
                下一篇：<%- page.next.title  %> >
            </small>
        </a>
    <% } %> 
</div>